<section class="keynotes">
	<h1 class="keynotes-title">{{homePt.keynotes.title}}</h1>
	{{#each homePt.keynotes.speakers}}
	<dl class="keynotes-speakers">
		<dd class="keynotes-speakers-photo">
			<img src="assets/img/keynotes/{{photo}}" alt="{{name}}">
		</dd>
		<dt class="keynotes-speakers-title">{{name}}</dt>
		<dd class="keynotes-speakers-subtitle">Keynote Speaker</dd>
		<dd class="keynotes-speakers-description">{{miniBio}}</dd>
		<dd>
			<a class="buttonAction" href="{{callToAction-url}}">{{callToAction}}</a>
		</dd>
	</dl>
	{{/each}}
</section>

<section class="program">
	<h1 class="program-title">{{homePt.program.title}}</h1>
	<p class="program-description">{{homePt.program.description}}</p>
	<a href="{{homePt.program.callToAction-url}}" class="buttonAction">{{homePt.program.callToAction}}</a>
</section>

<section class="place">
	<h1 class="place-title">{{homePt.place.title}}</h1>
	<p class="place-description">{{homePt.place.description}}</p>
	<a href="{{homePt.place.callToAction-url}}" class="buttonAction">{{homePt.place.callToAction}}</a>
</section>

<section class="sponsorship">
	<h1 class="sponsorship-title">{{homePt.sponsorship.title}}</h1>
	<p class="sponsorship-description">{{homePt.sponsorship.description}}</p>
	<a href="{{homePt.sponsorship.callToAction-url}}" class="buttonAction">{{homePt.sponsorship.callToAction}}</a>
</section>