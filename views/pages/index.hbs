---
title: '<%= i18n[language].seo.home.title %>'
description: '<%= i18n[language].seo.home.description %>'
body_class: '<%= i18n[language].seo.home.body_class %>'
---
<!DOCTYPE html>
<html lang="{{language}}">
  <head>
    <title>{{title}}</title>
    <meta charset="utf-8">
    <meta name="description" content="{{description}}">
    <meta name=viewport content="width=device-width,initial-scale=1">

    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,700,600,300,800' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="assets/css/reset.css">
    <link rel="stylesheet" type="text/css" href="assets/css/button.css">
    <link rel="stylesheet" href="assets/css/header.css">
    <link rel="stylesheet" href="assets/css/keynotes.css">
    <link rel="stylesheet" href="assets/css/program.css">
    <link rel="stylesheet" href="assets/css/place.css">
    <link rel="stylesheet" href="assets/css/sponsorship.css">
    <link rel="stylesheet" href="assets/css/footer.css">
    <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="assets/js/html5shiv.js"></script>
    <![endif]-->
  </head>

  <body class="{{body_class}}">

      {{> head }}

      {{> home }}

      {{> footer }}
  </body>
</html>
